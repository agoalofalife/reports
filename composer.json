{
    "name": "agoalofalife/reports",
    "description": "UI for created and download reports in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "agoalofalife",
            "email": "agoalofalife@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.9",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "agoalofalife\\Reports\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "agoalofalife\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure --no-coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "agoalofalife\\Reports\\ReportsServiceProvider"
            ]
        }
    }
}
